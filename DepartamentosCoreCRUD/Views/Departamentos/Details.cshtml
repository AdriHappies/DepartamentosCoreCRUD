@model Departamento

    <a asp-controller="Departamentos"
       asp-action="Index">
        Volver al listado
    </a>
    <hr />

    <ul class="list-group">
        <li class="list-group-item">
            Nombre: @Model.Nombre
        </li>
        <li class="list-group-item">
            Número: @Model.Numero
        </li>
        <li class="list-group-item">
            Localidad: @Model.Localidad
        </li>
    </ul>

